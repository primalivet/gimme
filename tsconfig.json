{
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "nodenext",                                
    "moduleResolution": "nodenext",                       
    "outDir": "./dist",                                   
    "rootDir": "./src",
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "noImplicitReturns": true,                        
    "noUncheckedIndexedAccess": true,                 
    "skipLibCheck": true
  },
}
